{
  "name": "quiz-me",
  "version": "1.0.0",
  "private": true,
  "main": "dist/bin/www.js",
  "scripts": {
    "start": "nodemon",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist/bin/www",
    "dev": "cross-env NODE_ENV=development npm-run-all build server",
    "prod": "cross-env NODE_ENV=production npm-run-all build server",
    "clean": "rimraf dist",
    "transpile": "babel ./server --out-dir dist --copy-files",
    "lint": "eslint ./server",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "mongoose": "^8.2.4",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/node": "^7.23.9",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4"
  }
}